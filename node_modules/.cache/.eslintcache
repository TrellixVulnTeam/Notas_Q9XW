lumn size={12}>\r\n              <Field>\r\n                <Label size=\"small\">Email:</Label>\r\n                <Control>\r\n                  <Input \r\n                    type=\"email\" \r\n                    required\r\n                    name=\"email\"\r\n                    value={email}\r\n                    onChange={e => setEmail(e.target.value)}\r\n                  />\r\n                </Control>\r\n              </Field>\r\n              <Field>\r\n                <Label size=\"small\">Password:</Label>\r\n                <Control>\r\n                  <Input \r\n                    type={passowordShown ? \"text\" : \"password\" }\r\n                    required\r\n                    name=\"password\"\r\n                    value={password}\r\n                    onChange={e => setPassword(e.target.value)}\r\n                  />\r\n                  <i className=\"password-icon\" onClick={togglePasswordVisiblity}>{passowordShown ? eye : eyeSlash}</i>\r\n                </Control>\r\n              </Field>\r\n              <Field>\r\n                <Control>\r\n                  <Column.Group breakpoint=\"mobile\">\r\n                    <Column>\r\n                      <a onClick={e => setRedirectToRegister(true)} className=\"button is-white has-text-custom-purple\">Registrar-se</a>\r\n                    </Column>\r\n                    <Column>\r\n                      <Button color=\"custom-purple\" outlined>Login</Button>\r\n                    </Column>\r\n                  </Column.Group>\r\n                </Control>\r\n              </Field>\r\n              { error && <Help color=\"danger\">Email ou Senha errado</Help> }\r\n            </Column>\r\n          </form>\r\n        </Column.Group>\r\n    </Fragment>\r\n    )\r\n}\r\n\r\nexport default LoginForm;","C:\\Users\\Rodrigo\\Desktop\\projetos\\notasReact\\javascript_note\\src\\components\\auth\\private_route\\index.js",[],"C:\\Users\\Rodrigo\\Desktop\\projetos\\notasReact\\javasc